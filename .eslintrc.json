{
    "extends": ["airbnb", "prettier"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "max-len": [2, 100],
        "class-methods-use-this": "off",
        "@typescript-eslint/no-unused-vars": 2,
        "indent": 0,
        "no-new": 0,
        "no-undef": 0,
        "no-plusplus": 0
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
